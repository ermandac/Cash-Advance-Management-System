{
  "development": {
    "username": "cams_user",
    "password": "your_password_here",
    "database": "cams_db",
    "host": "localhost",
    "port": 5432,
    "dialect": "postgres",
    "pool": {
      "max": 5,
      "min": 0,
      "acquire": 30000,
      "idle": 10000
    }
  },
  "production": {
    "username": "process.env.DB_USER",
    "password": "process.env.DB_PASSWORD",
    "database": "process.env.DB_NAME",
    "host": "process.env.DB_HOST",
    "port": "process.env.DB_PORT",
    "dialect": "postgres",
    "ssl": true,
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    },
    "pool": {
      "max": 20,
      "min": 5,
      "acquire": 60000,
      "idle": 10000
    }
  }
}
